{
  "name": "neonctl-test-release",
  "repository": {
    "type": "git",
    "url": "git@github.com:duskpoet/neonctl.git"
  },
  "type": "module",
  "version": "1.18.0",
  "description": "CLI tool for NeonDB Cloud management",
  "main": "index.js",
  "author": "NeonDB",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "neonctl": "cli.js"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@jest/globals": "^29.5.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/cli-table": "^0.3.0",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.7.13",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "emocks": "^3.0.1",
    "eslint": "^8.22.0",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^21.0.2",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@neondatabase/api-client": "1.1.0",
    "@segment/analytics-node": "^1.0.0-beta.26",
    "axios": "^1.4.0",
    "axios-debug-log": "^1.0.0",
    "chalk": "^5.2.0",
    "cli-table": "^0.3.11",
    "inquirer": "^9.2.6",
    "open": "^8.4.0",
    "openid-client": "^5.1.9",
    "yaml": "^2.1.1",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "watch": "tsc --watch",
    "lint": "tsc --noEmit && eslint src --ext .ts",
    "build": "npm run generateParams && npm run clean && tsc && cp src/*.html package*.json README.md ./dist",
    "clean": "rm -rf dist",
    "generateParams": "ts-node --esm generateOptionsFromSpec.ts",
    "start": "node src/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
